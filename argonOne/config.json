{
  "name": "ArgonOne Temp Control",
  "version": "0.1",
  "slug": "argon_one_temp",
  "description": "This adjusts the temp of ArgonOne by fan control",
  "arch": ["armhf", "armv7", "aarch64"],
  "startup": "application",
  "full_access": "no",
  "boot": "auto",
  "gpio": true,
  "devices": ["/dev/i2c-1:/dev/i2c-1:rwm"],
  "privileged": ["SYS_ADMIN"],
  "options": {
     "CorF": "F",
     "LowRange": 70,
     "MediumRange": 80,
     "HighRange": 90,
     "QuietProfile": true
  },
  "schema": {
    "CorF": "str",
    "LowRange": "int(0,255)",
    "MediumRange": "int(0,255)",
    "HighRange": "int(0,255)",
    "QuietProfile":"bool"
   }
}
